
# gcc toolchains
export PATH=$PATH:$HOME/chains/gcc-arm-none-eabi/bin
export PATH=$PATH:$HOME/chains/gcc-i586-elf/bin

# Android development extras
export ANDROID_HOME=/usr/local/opt/android-sdk

# Golang extras
export GOPATH=$HOME
#export PATH=$PATH:$GOPATH/bin

# grep extras
export GREP_OPTIONS='--color=auto'

# GPG extras
export LC_CTYPE="en_US.UTF-8"
export GPG_TTY=$(tty)

# Used to create clean tarballs
export COPYFILE_DISABLE=true

# Use pyvenv to manage multiple python environments
export PYENV_ROOT=/usr/local/opt/pyenv
source ~/.venv/venv/bin/activate

export RBENV_ROOT=/usr/local/var/rbenv
if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi

export BYOBU_PREFIX=$(brew --prefix)

# The next line updates PATH for the Google Cloud SDK.
source '/Users/amlweems/google-cloud-sdk/path.bash.inc'

# The next line enables bash completion for gcloud.
source '/Users/amlweems/google-cloud-sdk/completion.bash.inc'

export CLOUDSDK_PYTHON=$(which python2.7)
export PATH=$PATH:$HOME/go_appengine

# sage setup
export SAGE_ROOT=/Applications/sage

[ -f ~/.fzf.bash ] && source ~/.fzf.bash

